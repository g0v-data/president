    <td align="center"  valign="top"    ><table summary="排版表格"  width="100%" border="0" cellpadding="0" cellspacing="0">
      <tr >
        <td  rowspan="3" valign="top" background="/Portals/0/Skins/newskin/bg_left_line.jpg"   class="left" scope="col"><a title="左方相關連結區" class="blockone" accesskey="L" href="Default.aspx?tabid=1131" >:::</a><table summary="左側資料表格"  width="194" border="0" align="right" cellpadding="0" cellspacing="0">
          <tr>
            <td ><img class="left-banner" src="/Portals/0/Skins/newskin/leftmenutop03.png" width="194" height="72" alt="新聞與活動" /></td>
            </tr>
          <tr>
            <td align="right"><table summary="左側選單"  border="0"  cellpadding="0" cellspacing="0" class="wisemenu">
              <tr>
                <td valign="top">

<script language="JavaScript" type="text/javascript">
    function startList() {
        var version = parseFloat(navigator.appVersion.split("MSIE")[1]);
        //var IE6 = false/*@cc_on || @_jscript_version < 5.7@*/;
        var Mode = document.documentMode; // check IE8 Quirks 
        //alert(document.documentMode);
        if (version < 7 || Mode < 7) {
            // IE6
            suckerfish(placeOnTop, "ul", "wisemenu_h");
        }

        if (navigator.appName == "Microsoft Internet Explorer") {
            // IE
            suckerfish(hover_ie, "li", "wisemenu_h");
        }
        else {
            // FF
            suckerfish(hover_ff, "li", "wisemenu_h");
        }

        var obj = document.getElementById("wisemenu_v");
        if (obj != null) {
            suckerfish(focus_left, "a", "wisemenu_v");
            if (navigator.appName == "Microsoft Internet Explorer") {
                suckerfish(hover_ie, "li", "wisemenu_v");
            }
            else {
                suckerfish(hover_ff, "li", "wisemenu_v");
            }
        }

        if (! +[1, ]) {
            // IE6 Background Imag Cache
            document.execCommand("BackgroundImageCache", false, true);
        }

    }

    function suckerfish(type, tag, parentId) {
        var sfEls = (parentId == null) ? document.getElementsByTagName(tag) : document.getElementById(parentId).getElementsByTagName(tag);
        type(sfEls);
    }

    function hover_ie(sfEls) {
        for (var i = 0; i < sfEls.length; i++) {
            sfEls[i].onmouseover = function() {
                focus_hover(this);
            }
            sfEls[i].onmouseout = function() {
                blur_hover(this);
            }
            sfEls[i].onfocusin = function() {
                focus_hover(this);
            }
            sfEls[i].onfocusout = function() {
                blur_hover(this);
            }
        }
    }

    function hover_ff(sfEls) {
        for (var i = 0; i < sfEls.length; i++) {
            sfEls[i].onkeyup = function() {
                onkeydownUL(this);
            }
            sfEls[i].onkeypress = function() {
                onkeyupUL(this);
            }
        }
    }

    function focus_hover(sfEls) {
        sfEls.className = "over";
        // IE6 iframe 
        var oIFR = document.getElementById('ifr' + sfEls.id.replace("Item", "List").replace("CurrentParent", "List").replace("CurrentItem", "List").replace("Current", "List"));
        if (oIFR != null) { oIFR.style.display = "block"; }
    }

    function blur_hover(sfEls) {
        sfEls.className = sfEls.className.replace("over", "");
        // IE6 iframe 
        var oIFR = document.getElementById('ifr' + sfEls.id.replace("Item", "List").replace("CurrentParent", "List").replace("CurrentItem", "List").replace("Current", "List"));
        if (oIFR != null) { oIFR.style.display = "none"; }
    }

    function setclass(sfEls,css) {
        sfEls.className = css;
    }

    function onkeydownUL(sfEls) {
        // previous li set classname = ""
        if (sfEls.previousSibling.previousSibling != null) {
            setclass(sfEls.previousSibling.previousSibling,"");
        }
        // current li set classname = "over"
        setclass(sfEls, "over"); 
    }

    function onkeyupUL(sfEls) {
        var _get = false;
        // last li set classname = ""
        if (sfEls.nextSibling.nextSibling == null) {
            for (i = 0; i < sfEls.childNodes.length; i++) {
                node = sfEls.childNodes[i];
                if (node.nodeName == "UL") {
                    _get = true;
                }
            }
            if (!_get) {
                setclass(sfEls, "");
                setclass(sfEls.parentNode.parentNode, "");
                var obj = sfEls.parentNode.parentNode.parentNode.parentNode;
                if (obj.nodeName =="LI" && obj.nextSibling.nextSibling == null) {
                    setclass(obj, "");  
                } 
            }
        }
    }
    
    function focus_left(sfEls) {
        for (var i = 0; i < sfEls.length; i++) {
            sfEls[i].onfocus = function() {
                this.className += " left_hover";
            }
            sfEls[i].onblur = function() {
                this.className = this.className.replace(" left_hover", "");
            }
        }
    }

    function placeOnTop(oCont) {
        for (var i = 0; i < oCont.length; i++) {
            var oIFR = document.getElementById('ifr' + oCont[i].id);
            if (oIFR == null) {
                var oIFR = document.createElement('iframe');
                oIFR.id = 'ifr' + oCont[i].id;
                oIFR.style.top = '0px';
                oIFR.style.left = '0px';
                oIFR.style.filter = "progid:DXImageTransform.Microsoft.Alpha(opacity=0)";
                oIFR.scrolling = 'no';
                oIFR.frameBorder = 'no';
                oIFR.style.display = 'none';
                oIFR.style.position = 'absolute';
                oCont[i].parentNode.appendChild(oIFR);
            }

            oIFR.style.width = oCont[i].offsetWidth;
            oIFR.style.height = oCont[i].offsetHeight;
            oIFR.style.top = oCont[i].offsetTop + 'px';
            oIFR.style.left = oCont[i].offsetLeft + 'px';
            oIFR.style.zIndex = -1;

        }
    }

    window.onload = function() {
        startList();
    }

</script>

<div id="wisemenu_v" class="jqueryslidemenu">
<ul id="WiseMenuNav">
<li id="WiseMenuNavParentItem93"><div class="news93"><a id="WiseMenuNavParentLink93" href="/Default.aspx?tabid=93" title="每日活動行程" >每日活動行程</a></div></li>
</ul>

</div>
</td>

                </tr>
              </table></td>
            </tr>
          <tr>
            <td align="right" ><table   border="0" cellpadding="0" cellspacing="0" summary="左側網頁內容 " class="leftcontent">
              <tr>
                <td id="dnn_leftmenu_content" align="center" valign="top"><a name="1878"></a><a name="2405"></a><span id="dnn_ctr2405_ContentPane" class="DNNAlignleft"><!-- Start_Module_2405 --><div id="dnn_ctr2405_ModuleContent">
	<div id="dnn_ctr2405_HtmlModule_lblContent" class="Normal">
		<table width="100%" cellspacing="0" cellpadding="0" border="0" align="center">
    <tbody>
        <tr>
            <td valign="top" align="center" scope="col"><img width="194" height="45" border="0" alt="總統府也在這裡" name="Image5" src="/Portals/0/images/link/top.png" /></td>
